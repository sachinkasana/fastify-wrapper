    "@types/node": "^20.3.0",
    "eslint": "^8.42.0",
    "prettier": "^2.8.8",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@fastify/autoload": "^5.7.1",
